-- Combines orders (Invoice) with customer details.
SELECT 
    c.CustomerId,
    c.FirstName,
    c.LastName,
    o.InvoiceId,
    o.InvoiceDate,
    o.Total
FROM Invoice o
INNER JOIN Customer c ON o.CustomerId = c.CustomerId;

-- LEFT JOIN (Customers without Orders)
-- Finds customers who registered but haven't bought anything.
SELECT 
    c.FirstName,
    c.LastName,
    c.Email
FROM Customer c
LEFT JOIN Invoice o ON c.CustomerId = o.CustomerId
WHERE o.InvoiceId IS NULL;

-- Revenue per Product (High performing SKUs)
-- Joins Track (p) with InvoiceLine (il) to calculate sales.
SELECT 
    p.Name AS ProductName,
    SUM(il.UnitPrice * il.Quantity) AS TotalRevenue
FROM Track p
INNER JOIN InvoiceLine il ON p.TrackId = il.TrackId
GROUP BY p.TrackId
ORDER BY TotalRevenue DESC;

-- Category Revenue Strategy
-- Joins Genre -&gt; Track -&gt; InvoiceLine to see which Category earns most.
SELECT 
    g.Name AS CategoryName, 
    SUM(il.UnitPrice * il.Quantity) AS CategoryRevenue
FROM Genre g
INNER JOIN Track p ON g.GenreId = p.GenreId
INNER JOIN InvoiceLine il ON p.TrackId = il.TrackId
GROUP BY g.Name
ORDER BY CategoryRevenue DESC;

-- Conditional Filtering (Where Clause)
-- Example: Sales in USA during the year 2012.
SELECT 
    c.FirstName, 
    c.LastName, 
    o.InvoiceId, 
    o.Total, 
    o.BillingCountry
FROM Invoice o
INNER JOIN Customer c ON o.CustomerId = c.CustomerId
WHERE o.BillingCountry = 'USA' 
  AND o.InvoiceDate BETWEEN '2012-01-01' AND '2012-12-31';</sql><current_tab id="0"/></tab_sql></sqlb_project>
